{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lua",
            "request": "launch",
            "name": "Test Client",
            "console": "integratedTerminal",
            "luaVersion": "latest",
            "windows": {
                "luaexe": "${command:extensionPath}actboy168.lua-debug-1.53.0/bin/windows/lua-debug"
            },
            "linux": {
                "luaexe": "${command:extensionPath}actboy168.lua-debug-1.53.0/bin/linux/lua-debug",
            },
            "osx": {
                "luaexe": "${command:extensionPath}actboy168.lua-debug-1.53.0/bin/macos/lua-debug",
            },
            "outputCapture": [
            ],
            "program": "",
            "path": null,
            "cpath": null,
            "arg": [
                "4711"
            ]
        },
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Test Extension",
            "runtimeExecutable": "${execPath}",
            "sourceMaps": false,
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/extension/"
            ],
            "env": {
                "VSCODE_EXTENSION_PATH" : "${command:extensionPath}actboy168.lua-debug-1.53.0"
            }
        },
        {
            "name": "test launch",
            "type": "lua",
            "request": "launch",
            "stopOnEntry": true,
            "program": "${workspaceFolder}/examples/standalone/test.lua",
            "cwd": "${workspaceFolder}",
            "path": "${workspaceFolder}/?.lua",
            "cpath": "${workspaceFolder}/?.dll",
            "arg": [],
            "consoleCoding": "utf8",
            "luaArch": "x86_64"
        },
        {
            "name": "Launch",
            "type": "cppvsdbg",
            "request": "attach",
            "processId":"${command:pickProcess}",
        }
    ]
}